		"babel-preset-es2015": "6.24.1",
		"babel-preset-es2017": "6.24.1",
		"babel-preset-react": "6.24.1",
		"babel-preset-stage-0": "6.24.1",
		"compression": "^1.7.4",
		"concurrently": "3.5.0",
		"express": "^4.17.1",
		"express-http-proxy": "1.0.6",
		"lodash": "^4.17.19",
		"nodemon": "^2.0.4",
		"npm-run-all": "4.1.1",
		"react": "^16.13.1",
